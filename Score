					end)
				end
			end
		end)
	end,
	["CAMPING AT BASE"] = function(p96)
		local v97 = workspace.NPCFolders.BaseFolder:FindFirstChild("Blue Base")
		if v97 then
			while task.wait(1) do
				local v98 = 0
				for _, v99 in ipairs(CollectionService:GetTagged("Friendly")) do
					if v99.PrimaryPart and (not CollectionService:HasTag(v99, "Dead") and (v97.PrimaryPart.Position - v99.PrimaryPart.Position).Magnitude <= 6) then
						v98 = v98 + 1
					end
				end
				if v98 >= 8 and AddMultiplierType(p96) == true then
					SFX.CamperAlert:Play()
				end
			end
		end
	end,
	["HAPPY BIRTHDAY"] = function(p_u_100)
		CollectionService:GetInstanceAddedSignal("Friendly"):Connect(function(p_u_101)
			if p_u_101.Name == "Birthday Battler" then
				local v_u_102 = false
				local v_u_103 = task.spawn(function()
					while p_u_101 and v_u_102 == false do
						if AddMultiplierType(p_u_100) == true then
							SFX.HappyBirthday:Play()
						end
						task.wait(0.5)
					end
				end)
				local v_u_104 = p_u_101:WaitForChild("Humanoid")
				if v_u_104 then
					v_u_104.HealthChanged:Connect(function(_)
						if v_u_102 ~= true then
							if v_u_104.Health <= 0 then
								v_u_102 = true
								task.cancel(v_u_103)
								if AddMultiplierType("BIRTHDAY RUINED") == true then
									SFX.BirthdayFail:Play()
								end
							end
						end
					end)
				end
			end
		end)
	end,
