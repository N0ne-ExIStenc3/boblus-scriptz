	["GETTING BORED"] = function(p111)
		local v_u_112 = 0
		CollectionService:GetInstanceAddedSignal("Dead"):Connect(function(_)
			v_u_112 = 0
		end)
		local v113 = v_u_112
		while task.wait(1) do
			v_u_112 = v113 + 1
			if v_u_112 >= 10 and AddMultiplierType(p111) == true then
				SFX.Snore:Play()
				v113 = v_u_112
			else
				v113 = v_u_112
			end
		end
	end
}
local v_u_114 = v_u_5
local v_u_115 = {}
for v116, v117 in pairs(MultiplierConditions) do
	v_u_115[v116] = task.spawn(v117, v116)
end
local v_u_118 = false
ScreenGui:GetAttributeChangedSignal("Win"):Connect(function()
	if v_u_118 ~= true then
		v_u_118 = true
		task.delay(0.5, function()
			CollectionService:AddTag(ScreenGui, "CreateScene")
		end)
		for _, v119 in pairs(v_u_115) do
			task.cancel(v119)
		end
		for _, v120 in ipairs(MultiplierList:GetChildren()) do
			if v120:IsA("Frame") then
				v120:Destroy()
			end
		end
		if ScreenGui:GetAttribute("Win") == true then
			if AddMultiplierType("VICTORY") == true then
				v_u_114 = true
				SFX.Victory:Play()
				return
			end
		elseif AddMultiplierType("LOSER") == true then
			v_u_114 = true
			SFX.Loser:Play()
			ScoreText.Text = 0
		end
	end
end)
MultiplierList.ChildAdded:Connect(function(p_u_121)
	CountAllMultiplier()
	local v_u_122 = p_u_121:GetAttributeChangedSignal("AddedMultiplier"):Connect(function()
		if p_u_121:GetAttribute("AddedMultiplier") then
			CountAllMultiplier()
		end
	end)
	p_u_121.Destroying:Connect(function()
		v_u_122:Disconnect()
	end)
end)
MultiplierList.ChildRemoved:Connect(function(_)
	CountAllMultiplier()
end)
