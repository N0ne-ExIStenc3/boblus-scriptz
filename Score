				if AddMultiplierType(p_u_92) == true then
					SFX.Gambling:Play()
				end
				local v94 = p93:WaitForChild("HumanoidRootPart")
				if v94 then
					v94.ChildAdded:Connect(function(p95)
						if p95.Name == "Bang" then
							if AddMultiplierType("BIG WIN") == true then
								SFX.Gamblecore:Play()
								return
							end
						elseif p95.Name == "Dud" and AddMultiplierType("AW DANG IT") == true then
							SFX.DangIt:Play()
