TweenService = game:GetService("TweenService")
CollectionService = game:GetService("CollectionService")
SoundService = game:GetService("SoundService")
GameValues = require(game.ReplicatedStorage.Modules.GameValues)
ScreenGui = game:GetService("Players").LocalPlayer.PlayerGui:WaitForChild("Scoring")
Assets = ScreenGui.Assets
MultiplierList = ScreenGui.MultiplierList
SFX = ScreenGui.SFX
ScoreText = ScreenGui.ScoreText
RandomNum = Random.new()
TweenService:Create(MultiplierList, TweenInfo.new(0.25, Enum.EasingStyle.Circular, Enum.EasingDirection.In, -1, true), {
	["Position"] = UDim2.fromScale(0.5, 0.58),
	["Size"] = UDim2.fromScale(1.1, 0.9)
}):Play()
Score = 0
CurrentMultiplier = 1
Tier = 1
CurrentTier = 1
StarMultiplier = GameValues.Stars
ScoreText:SetAttribute("TextColor3", ScoreText.TextColor3)
Camera = workspace.CurrentCamera
local v1 = {
    ["OBLITERATED"] = {
        ["AddedMultiplier"] = -2,
    },
	["KILLSTREAK"] = {
		["AddedMultiplier"] = 0.05,
		["Stack"] = math.huge
