MultiplierType = v1
function FlashMultiplierFrame(p2)
	local v3 = p2:GetAttribute("Size").X.Scale
	local v4 = p2:GetAttribute("Size").Y.Scale
	p2.Size = UDim2.fromScale(v3 * 1.2, v4 * 1.2)
	p2.TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	TweenService:Create(p2, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
		["Size"] = UDim2.fromScale(v3, v4)
	}):Play()
	TweenService:Create(p2.TextLabel, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
		["TextColor3"] = p2:GetAttribute("TextColor3")
	}):Play()
end
local v_u_5 = false
function AddMultiplierType(p6)
	if v_u_5 == true then
		return
	else
		CountAllMultiplier()
		local v7 = MultiplierList:FindFirstChild(p6)
		if v7 then
			FlashMultiplierFrame(v7)
			if MultiplierType[p6].Stack and MultiplierType[p6].Stack > v7:GetAttribute("Stack") then
				v7:SetAttribute("Stack", v7:GetAttribute("Stack") + 1)
				v7:SetAttribute("AddedMultiplier", v7:GetAttribute("AddedMultiplier") + MultiplierType[p6].AddedMultiplier)
			end
			local v8 = p6 .. (not (MultiplierType[p6].Stack and v7:GetAttribute("Stack")) and " " or " [x" .. v7:GetAttribute("Stack") .. "] ") .. "[" .. MultiplierType[p6].AddedMultiplier * 100 * v7:GetAttribute("Stack") * StarMultiplier .. "%]"
			v7.TextLabel.Text = v8
			v7:SetAttribute("Time", MultiplierType[p6].Time or 5)
		elseif MultiplierType[p6] then
			local v_u_9
			if MultiplierType[p6].AddedMultiplier >= 0 then
				v_u_9 = Assets.MultiplierFrame:Clone()
			else
				v_u_9 = Assets.MultiplierFrameBad:Clone()
			end
			local v10 = p6 .. (MultiplierType[p6].Stack and " [x1] " or " ") .. "[" .. MultiplierType[p6].AddedMultiplier * 100 * StarMultiplier .. "%]"
			v_u_9:SetAttribute("Stack", 1)
			v_u_9:SetAttribute("TextColor3", v_u_9.TextLabel.TextColor3)
			v_u_9:SetAttribute("Size", v_u_9.Size)
			v_u_9:SetAttribute("AddedMultiplier", MultiplierType[p6].AddedMultiplier)
			v_u_9.Name = p6
			v_u_9.TextLabel.Text = v10
			v_u_9.Parent = MultiplierList
			v_u_9.Visible = true
			v_u_9:SetAttribute("Time", MultiplierType[p6].Time or 5)
			FlashMultiplierFrame(v_u_9)
			local v_u_21 = task.spawn(function()
				while v_u_9 do
					local v11 = TweenService
					local v12 = v_u_9.TextLabel
					local v13 = TweenInfo.new(0.05)
					local v14 = {
						["Rotation"] = RandomNum:NextNumber(0, 3)
					}
					local v15 = v11:Create(v12, v13, v14)
					v15:Play()
					v15.Completed:Wait()
					local v16 = TweenService
					local v17 = v_u_9.TextLabel
					local v18 = TweenInfo.new(0.05)
					local v19 = {
						["Rotation"] = RandomNum:NextNumber(-3, 0)
					}
					local v20 = v16:Create(v17, v18, v19)
					v20:Play()
					v20.Completed:Wait()
				end
			end)
			task.spawn(function()
				local _ = v_u_9:GetAttribute("Size").X.Scale
				local _ = v_u_9:GetAttribute("Size").Y.Scale
				local v_u_22 = TweenService:Create(v_u_9.TextLabel, TweenInfo.new(0.05, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, -1, true), {
					["TextColor3"] = Color3.fromRGB(255, 255, 0)
				})
				v_u_9:GetAttributeChangedSignal("Time"):Connect(function()
					if v_u_9:GetAttribute("Time") > 1 then
						v_u_9.Despawning:Stop()
						v_u_22:Pause()
						v_u_9.TextLabel.TextColor3 = v_u_9:GetAttribute("TextColor3")
					end
				end)
				while task.wait(1) and (v_u_9 and v_u_9.Name ~= "Worthless") do
					v_u_9:SetAttribute("Time", v_u_9:GetAttribute("Time") - 1)
					if v_u_9:GetAttribute("Time") <= 1 and v_u_9:FindFirstChild("Despawning") then
						v_u_9.Despawning:Play()
						v_u_22:Play()
					end
					if v_u_9:GetAttribute("Time") <= 0 and v_u_9:FindFirstChild("Despawning") then
						v_u_9.Despawning:Stop()
						v_u_9.Name = "Worthless"
						v_u_9:SetAttribute("AddedMultiplier", 0)
						if v_u_9.TextLabel:FindFirstChild("GlowEffect") then
							v_u_9.TextLabel.GlowEffect.Visible = false
						end
						TweenService:Create(v_u_9, TweenInfo.new(0.5), {
							["Size"] = UDim2.fromScale(v_u_9:GetAttribute("Size").X.Scale, 0)
						}):Play()
						TweenService:Create(v_u_9.TextLabel, TweenInfo.new(0.5), {
							["TextTransparency"] = 1
						}):Play()
						TweenService:Create(v_u_9.TextLabel.UIStroke, TweenInfo.new(0.5), {
							["Transparency"] = 1
						}):Play()
						SFX.Vanish:Play()
						task.wait(0.5)
						if v_u_9 then
							v_u_9:Destroy()
						end
					end
				end
			end)
			v_u_9.Destroying:Connect(function()
				task.cancel(v_u_21)
			end)
			return true
		end
	end
end
local v_u_23 = ScoreText.Position
local v_u_24 = ScoreText.Size
function ChangeScoreTween()
	local v25 = 0.6 - Tier * 0.1
	ScoreText.Position = v_u_23
	ScoreText.Size = v_u_24
	TweenService:Create(ScoreText, TweenInfo.new(v25, Enum.EasingStyle.Circular, Enum.EasingDirection.InOut, -1, true), {
		["Position"] = v_u_23 + UDim2.fromScale(0, 0.015),
		["Size"] = UDim2.fromScale(v_u_24.X.Scale * 1.4, v_u_24.Y.Scale * 1.25)
	}):Play()
end
ChangeScoreTween()
function CheckTier()
	if 1 + Tier <= CurrentMultiplier and (Tier < 5 and Tier < 5) then
		Tier = Tier + 1
		CheckTier()
	end
	if Tier > CurrentMultiplier and Tier > 1 then
		Tier = Tier - 1
		CheckTier()
		if Tier <= 1 then
			Tier = 1
		end
	end
	if CurrentTier ~= Tier then
		CurrentTier = Tier
		ChangeScoreTween()
	end
end
function CountAllMultiplier()
	CurrentMultiplier = 1
	for _, v26 in ipairs(MultiplierList:GetChildren()) do
		if v26:GetAttribute("AddedMultiplier") then
			CurrentMultiplier = CurrentMultiplier + v26:GetAttribute("AddedMultiplier") * StarMultiplier
		end
	end
	CheckTier()
	local v27 = ScoreText.Multiplier
	local v28 = CurrentMultiplier * 100
	v27.Text = math.floor(v28) / 100 .. "x"
end
if StarMultiplier == 2 then
	Assets.MultiplierFrame.TextLabel.TextColor3 = Color3.fromRGB(255, 246, 114)
	Assets.MultiplierFrame.TextLabel.UIStroke.Color = Color3.fromRGB(171, 137, 1)
	Assets.MultiplierFrameBad.TextLabel.TextColor3 = Color3.fromRGB(117, 103, 0)
	Assets.MultiplierFrameBad.TextLabel.UIStroke.Color = Color3.fromRGB(33, 31, 21)
	ScoreText.Multiplier.TextColor3 = Color3.fromRGB(255, 246, 114)
	ScoreText.Multiplier.UIStroke.Color = Color3.fromRGB(171, 137, 1)
elseif StarMultiplier == 3 then
	Assets.MultiplierFrame.TextLabel.TextColor3 = Color3.fromRGB(255, 120, 120)
	Assets.MultiplierFrame.TextLabel.UIStroke.Color = Color3.fromRGB(171, 0, 0)
	Assets.MultiplierFrameBad.TextLabel.TextColor3 = Color3.fromRGB(117, 0, 0)
	Assets.MultiplierFrameBad.TextLabel.UIStroke.Color = Color3.fromRGB(33, 22, 22)
	ScoreText.Multiplier.TextColor3 = Color3.fromRGB(255, 120, 120)
	ScoreText.Multiplier.UIStroke.Color = Color3.fromRGB(171, 0, 0)
end
local v_u_29 = Instance.new("BloomEffect")
CollectionService:AddTag(v_u_29, "ScoreCleanup")
v_u_29.Size = Tier * 6
v_u_29.Intensity = Tier * 0.4
v_u_29.Threshold = 2.3 - Tier * 0.3
v_u_29.Parent = game.Lighting
function FlashScreen()
	Camera.FieldOfView = 70 + Tier * 2
	v_u_29.Size = Tier * 6
	v_u_29.Intensity = Tier * 0.4
	v_u_29.Threshold = 2.3 - Tier * 0.3
	TweenService:Create(Camera, TweenInfo.new(0.6, Enum.EasingStyle.Quad), {
		["FieldOfView"] = 70
	}):Play()
	TweenService:Create(v_u_29, TweenInfo.new(0.6, Enum.EasingStyle.Quad), {
		["Size"] = 0,
		["Intensity"] = 0,
		["Threshold"] = 2
	}):Play()
end
ScreenGui.Destroying:Connect(function()
	for _, v30 in ipairs(CollectionService:GetTagged("ScoreCleanup")) do
		v30:Destroy()
	end
end)
function CreateParticles(p31, _, _, p32)
	if Tier >= 2 then
		local v33 = Assets.PointLight:Clone()
		v33.Parent = p31
		local v34 = Assets.Sparkle:Clone()
		v34.Parent = p31
		if p32 == "ScoreGivenBig" then
			TweenService:Create(v33, TweenInfo.new(0.75, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, true), {
				["Range"] = 12,
				["Brightness"] = 2
			}):Play()
			v34.Size = NumberSequence.new(6)
			v34.Color = ColorSequence.new(Color3.fromRGB(255, 255, 0))
			v34.Lifetime = NumberRange.new(0.7)
			v34:Emit(6)
		elseif p32 == "ScoreGivenMassive" then
			TweenService:Create(v33, TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, true), {
				["Range"] = 15,
				["Brightness"] = 4
			}):Play()
			v34.Size = NumberSequence.new(8)
			v34.Color = ColorSequence.new(Color3.fromRGB(255, 0, 0))
			v34.Lifetime = NumberRange.new(1.1)
			v34:Emit(10)
		else
			TweenService:Create(v33, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, true), {
				["Range"] = 6,
				["Brightness"] = 1.5
			}):Play()
			v34:Emit(4)
		end
	end
	if Tier >= 3 then
		local v35 = Assets.KillSounds:GetChildren()[math.random(1, #Assets.KillSounds:GetChildren())]:Clone()
		v35.Parent = p31
		v35.PlaybackSpeed = RandomNum:NextNumber(0.8, 1.5)
		v35:Play()
		local v36 = Assets.PointLight:Clone()
		v36.Parent = p31
		local v37 = Assets.Smoke:Clone()
		v37.Parent = p31
		if p32 == "ScoreGivenBig" then
			v36.Color = Color3.fromRGB(255, 255, 0)
			TweenService:Create(v36, TweenInfo.new(0.75, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, true), {
				["Range"] = 14,
				["Brightness"] = 3
			}):Play()
			v37.Color = ColorSequence.new(Color3.fromRGB(255, 255, 0))
			v37.Size = NumberSequence.new(9)
			v37.Lifetime = NumberRange.new(0.75)
			v37:Emit(6)
		elseif p32 == "ScoreGivenMassive" then
			v36.Color = Color3.fromRGB(255, 0, 0)
			TweenService:Create(v36, TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, true), {
				["Range"] = 20,
				["Brightness"] = 6
			}):Play()
			v37.Color = ColorSequence.new(Color3.fromRGB(255, 0, 0))
			v37.Size = NumberSequence.new(12)
			v37.Lifetime = NumberRange.new(1)
			v37:Emit(10)
		else
			TweenService:Create(v36, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, true), {
				["Range"] = 8,
				["Brightness"] = 2
			}):Play()
			v37:Emit(4)
		end
	end
	if Tier >= 5 then
		if p32 == "ScoreGivenMassive" then
			local v38 = Assets.PointLight:Clone()
			v38.Parent = p31
			v38.Color = Color3.fromRGB(255, 155, 0)
			TweenService:Create(v38, TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, true), {
				["Range"] = 30,
				["Brightness"] = 10
			}):Play()
			for _, v39 in ipairs(Assets.BossExplosion:GetChildren()) do
				local v40 = v39:Clone()
				v40.Parent = p31
				if v40:IsA("Sound") then
					v40:Play()
				end
				if v40:IsA("ParticleEmitter") then
					v40:Emit(30)
				end
			end
			return
		end
		if p32 == "ScoreGivenBig" then
			local v41 = Assets.PointLight:Clone()
			v41.Parent = p31
			v41.Color = Color3.fromRGB(255, 155, 0)
			TweenService:Create(v41, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, true), {
				["Range"] = 15,
				["Brightness"] = 6
			}):Play()
			local v42 = Assets.Explosion:Clone()
			v42.Parent = p31
			v42.Size = NumberSequence.new(9)
			v42:Emit(8)
			local v43 = Assets.Boom:Clone()
			v43.Parent = p31
			v43.PlaybackSpeed = v43.PlaybackSpeed * 0.8
			v43.Volume = v43.Volume * 2
			v43:Play()
			return
		end
		local v44 = Assets.PointLight:Clone()
		v44.Parent = p31
		v44.Color = Color3.fromRGB(255, 155, 0)
		TweenService:Create(v44, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, true), {
			["Range"] = 10,
			["Brightness"] = 4
		}):Play()
		local v45 = Assets.Explosion:Clone()
		v45.Parent = p31
		v45.Size = NumberSequence.new(6)
		v45.Speed = NumberRange.new(13)
		v45:Emit(5)
		local v46 = Assets.Boom:Clone()
		v46.Parent = p31
		v46:Play()
	end
end
function AddPoints(p47, p48, p49, p50)
	task.spawn(FlashScreen)
	local v51 = p48 * CurrentMultiplier * StarMultiplier
	local v52 = math.floor(v51)
	ScoreText.TextColor3 = Color3.fromRGB(255, 255, 255)
	Score = Score + v52
	if Score <= 0 then
		Score = 0
	end
	ScoreText.Text = Score
	TweenService:Create(ScoreText, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
		["TextColor3"] = ScoreText:GetAttribute("TextColor3")
	}):Play()
	local v_u_53 = Instance.new("Attachment")
	v_u_53.Name = "ScoreAttachment"
	v_u_53.WorldPosition = p50
	v_u_53.Parent = workspace.Terrain
	CollectionService:AddTag(v_u_53, "ScoreCleanup")
	CreateParticles(v_u_53, p47, p48, p49)
	local v_u_54 = Assets:FindFirstChild(p49):Clone()
	local v55 = v_u_54.Frame.TextLabel.TextColor3
	v_u_54.Frame.TextLabel.Text = v52
	v_u_54.StudsOffsetWorldSpace = Vector3.new(0, -1.5, 0)
	v_u_54.Frame.TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	local v56 = RandomNum:NextNumber(-15, 15)
	v_u_54.Frame.Rotation = v56
	v_u_54.Enabled = true
	v_u_54.Parent = v_u_53
	TweenService:Create(v_u_54.Frame.TextLabel, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
		["TextColor3"] = v55
	}):Play()
	TweenService:Create(v_u_54, TweenInfo.new(v_u_54:GetAttribute("Duration")), {
		["StudsOffsetWorldSpace"] = Vector3.new(0, 1.5, 0)
	}):Play()
	TweenService:Create(v_u_54.Frame, TweenInfo.new(RandomNum:NextNumber(0.05, 0.15), Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, -1, true), {
		["Rotation"] = -v56
	}):Play()
	task.delay(v_u_54:GetAttribute("Duration") - 0.5, function()
		TweenService:Create(v_u_54.Frame.TextLabel, TweenInfo.new(0.5), {
			["TextTransparency"] = 1
		}):Play()
		TweenService:Create(v_u_54.Frame.TextLabel.UIStroke, TweenInfo.new(0.5), {
			["Transparency"] = 1
		}):Play()
		task.wait(10)
		v_u_53:Destroy()
	end)
end
MultiplierConditions = {
	["KILLSTREAK"] = function(p_u_57)
		local v_u_58 = {}
		CollectionService:GetInstanceAddedSignal("Boss"):Connect(function(p59)
			local v60 = v_u_58
			table.insert(v60, p59)
		end)
		CollectionService:GetInstanceAddedSignal("Dead"):Connect(function(p61)
			if CollectionService:HasTag(p61, "Enemy") then
				local v62 = p61.PrimaryPart.Position
				local v63
				if p61:FindFirstChild("SFX") and (p61.SFX:FindFirstChild("Spawn") and (p61:GetAttribute("Health") >= 100 and not table.find(v_u_58, p61))) then
					v63 = "ScoreGivenBig"
					if AddMultiplierType("SLAINED MINIBOSS") == true then
						SFX.Death:Play()
					end
				else
					v63 = "ScoreGiven"
				end
				if table.find(v_u_58, p61) then
					v63 = "ScoreGivenMassive"
					if AddMultiplierType("BOSS DEFEATED") == true then
						SFX.Airhorn:Play()
					end
				end
				local v64 = p61:GetAttribute("Health")
				if v64 then
					if p61:GetAttribute("Summoned") then
						v64 = v64 * 0.35
					end
					AddPoints(p61, v64, v63, v62)
				end
				if CurrentMultiplier > 0 then
					local v65 = 1
					if MultiplierList:FindFirstChild(p_u_57) then
						v65 = v65 + MultiplierList:FindFirstChild(p_u_57):GetAttribute("Stack") * 0.03
					end
					SFX.Kill.PlaybackSpeed = v65
					SFX.Kill:Play()
				else
					SFX.BadScore:Play()
				end
				AddMultiplierType(p_u_57)
			end
		end)
	end,
	["CLICKED"] = function(p_u_66)
		game:GetService("Players").LocalPlayer:GetMouse().Button1Down:Connect(function()
			local v67 = 1
			if MultiplierList:FindFirstChild(p_u_66) then
				v67 = v67 + MultiplierList:FindFirstChild(p_u_66):GetAttribute("Stack") * 0.03
			end
			SFX.Click.PlaybackSpeed = v67
			SFX.Click:Play()
			AddMultiplierType(p_u_66)
		end)
	end,
	["SPAWNED UNIT"] = function(p_u_68)
